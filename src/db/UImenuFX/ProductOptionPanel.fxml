<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="account_window" prefHeight="200.0" prefWidth="370.0" styleClass="border_color" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="db.UImenuFX.ProductOptionController">
   <children>
      <Pane layoutX="5.0" layoutY="5.0" prefHeight="190.0" prefWidth="360.0" styleClass="white_color">
         <children>
            <JFXButton fx:id="newProduct_button" layoutX="29.0" layoutY="84.0" prefHeight="74.0" prefWidth="113.0" styleClass="charging_screen_bg_color" text="New product" textFill="#4f90a5">
               <font>
                  <Font name="Ebrima Bold" size="14.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="orderProduct_button" layoutX="196.0" layoutY="84.0" prefHeight="74.0" prefWidth="133.0" styleClass="starter_small_buttom" text="Order product" textFill="#4f90a5">
               <font>
                  <Font name="Ebrima Bold" size="14.0" />
               </font>
            </JFXButton>
         </children>
      </Pane>
      <Pane layoutX="6.0" prefHeight="55.0" prefWidth="354.0">
         <children>
            <ImageView fitHeight="55.0" fitWidth="360.0" layoutY="5.0">
               <image>
                  <Image url="@src.IconPictures/UpperBarSmall.png" />
               </image>
            </ImageView>
            <ImageView fx:id="exit_button" fitHeight="28.0" fitWidth="40.0" layoutX="321.0" layoutY="14.0" onMouseClicked="#close_window" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@src.IconPictures/window-close.png" />
               </image>
            </ImageView>
            <Label layoutX="71.0" layoutY="21.0" text="Inventory">
               <font>
                  <Font name="Ebrima Bold" size="18.0" />
               </font>
            </Label>
            <ImageView fitHeight="55.0" fitWidth="57.0" layoutY="8.0" preserveRatio="true">
               <image>
                  <Image url="@src.IconPictures/logoNegro.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>
